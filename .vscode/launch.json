{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug app",
      "type": "go",
      "request": "attach",
      "debugAdapter": "legacy",
      "mode": "remote",
      "remotePath": "${workspaceFolder}",
      "port": 2347,
      "host": "127.0.0.1",
      "substitutePath": [
        {
          "from": "/Users/l/Projects/varso/",
          "to": "/app/"
        }
      ],
      "showLog": true,
      "apiVersion": 2,
      "trace": "verbose"
    },
    {
      "name": "Debug iOS Hermes",
      "cwd": "${workspaceFolder}",
      "type": "reactnativedirect",
      "request": "launch",
      "platform": "ios"
    },
    {
      "name": "Run iOS Hermes",
      "cwd": "${workspaceFolder}",
      "type": "reactnativedirect",
      "request": "launch",
      "platform": "ios",
      "enableDebug": false
    }
  ]
}
